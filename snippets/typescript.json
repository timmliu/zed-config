{
 "Print to console w/ label newline": {
		"prefix": "ll",
		"body": [
			"console.log(`---$1`) // !!!",
			"console.log($1) // !!!"
		],
		"description": "console.log w/ label newline"
	},
	"Print to console w/ label same line": {
		"prefix": "cc",
		"body": [
			"console.log(`---$1 =>`, $1) // !!!"
		],
		"description": "console.log w/ label same line"
	},
  "Throw Exception": {
    "prefix": "throw",
    "body": ["throw new Error(\"$1\");", "$0"],
    "description": "Throw Exception"
  }
}

